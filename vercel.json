{
    "version": 2,
    "buildCommand": null,
    "outputDirectory": ".",
    "rewrites": [
      { "source": "/", "destination": "/index.html" },
      
      { "source": "/app", "destination": "/app/index.html" },
      { "source": "/app/dashboard", "destination": "/app/dashboard.html" },
      { "source": "/app/login", "destination": "/app/login.html" },
      { "source": "/app/wallet", "destination": "/app/wallet.html" },
      { "source": "/app/transactions", "destination": "/app/transactions.html" },
      { "source": "/app/services/payments", "destination": "/app/services/payments.html" },
      { "source": "/app/services/investments", "destination": "/app/services/investments.html" },
      { "source": "/app/services/staking", "destination": "/app/services/staking.html" },
      { "source": "/app/profile", "destination": "/app/profile.html" },
      { "source": "/app/settings", "destination": "/app/settings.html" },
      { "source": "/app/support", "destination": "/app/support.html" },
      
      { "source": "/digital_access", "destination": "/digital_access/digital_access.html" },
      { "source": "/digital_access/corporate_treasury", "destination": "/digital_access/corporate_treasury/corporate_treasury.html" },
      { "source": "/digital_access/digital_wallet", "destination": "/digital_access/digital_wallet/digital_wallet.html" },
      { "source": "/digital_access/digital_card", "destination": "/digital_access/digital_card/digital_card.html" },
      { "source": "/digital_access/point_of_sale", "destination": "/digital_access/point_of_sale/point_of_sale.html" },
      { "source": "/digital_access/private_brokerage", "destination": "/digital_access/private_brokerage/private_brokerage.html" },
      { "source": "/digital_access/private_brokerage/calculator", "destination": "/digital_access/private_brokerage/calculator/calculator.html" },
    
      { "source": "/digital_financial_services", "destination": "/digital_financial_services/digital_financial_services.html" },
      { "source": "/digital_financial_services/payroll", "destination": "/digital_financial_services/payrrol/payrrol.html" },
      { "source": "/digital_financial_services/cross_border", "destination": "/digital_financial_services/cross_border/cross_border.html" },
      { "source": "/digital_financial_services/custody_solutions", "destination": "/digital_financial_services/custody_solutions/custody_solutions.html" },
      { "source": "/digital_financial_services/asset_management", "destination": "/digital_financial_services/asset-management/asset-management.html" },
      { "source": "/digital_financial_services/rwa_tokenization", "destination": "/digital_financial_services/rwa-tokenization/rwa-tokenization.html" },
      { "source": "/digital_financial_services/stablecoins", "destination": "/digital_financial_services/Stablecoins/stablecoins.html" },
      { "source": "/digital_financial_services/liquidity_provision", "destination": "/digital_financial_services/Stablecoins/liquidity-provision.html" },
      
      { "source": "/privacy_policy", "destination": "/lega_docs/Privacy_Policy/Privacy_policy.html" },
      { "source": "/terms_conditions", "destination": "/lega_docs/Terms_conditions/Terms_conditions.html" },
      { "source": "/amlft_policy", "destination": "/lega_docs/AMLFT_policy/amlft_policy.html" },
      
      { "source": "/about_us", "destination": "/about_us/about_us.html" },
      { "source": "/kyc", "destination": "/kyc-page/kyc.html" },
      
      { "source": "/(.*)", "destination": "/index.html" }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "X-Content-Type-Options",
            "value": "nosniff"
          },
          {
            "key": "X-Frame-Options",
            "value": "DENY"
          },
          {
            "key": "X-XSS-Protection",
            "value": "1; mode=block"
          }
        ]
      }
    ]
}